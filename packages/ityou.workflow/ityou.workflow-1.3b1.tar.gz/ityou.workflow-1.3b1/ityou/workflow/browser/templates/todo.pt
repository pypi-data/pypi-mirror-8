<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      lang="en"
      metal:use-macro="here/main_template/macros/master"
      i18n:domain="ityou.workflow">
<body>
    <div metal:fill-slot="main">
    <metal:main-macro define-macro="main"
                      tal:define="kssClassesView context/@@kss_field_decorator_view;
                                  getKssClasses nocall:kssClassesView/getKssClassesInlineEditable;
                                  templateId template/getId;
                                  is_mobile python:False;
                                  ">
                                  
	    <div id ="todo-list-in">
	        <form>
            <table id ="todo-table">
                <thead>
                    <tr>
                        <th class="portrait"></th>                        
                        <th class="sender" i18n:translate="sender">Sender</th>
                        <th class="url" i18n:translate="title">Title</th>
                        <th class="description" i18n:translate="description">Description</th>
                        <th class="update" i18n:translate="update">Last update</th>
                        <!--
                        <th class="deadline" i18n:translate="deadline">Deadline</th>
                        -->
                        <th class="state" i18n:translate="state">Set state</th>
                        <!--
                        <th class="oid" i18n:translate="id">ID</th>
                        <th class="priority"></th>
                        -->                  
                    </tr>
                </thead>   
                <tbody>
                </tbody>
            </table>
            </form>
        </div>            
    
    </metal:main-macro>
    </div>
</body>
</html>
