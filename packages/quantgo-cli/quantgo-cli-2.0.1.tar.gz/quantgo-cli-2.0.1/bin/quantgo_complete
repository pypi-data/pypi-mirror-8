#!/usr/bin/env python
import os
import sys

# add parent dir to be able to import from qnet_cli package
parent_dir = os.path.dirname(os.path.dirname(os.path.abspath(__file__)))
sys.path.append(parent_dir)

import qg_cli.completer

if __name__ == '__main__':
    cline = os.environ.get('COMP_LINE') or ''
    cpoint = int(os.environ.get('COMP_POINT') or len(cline))
    qg_cli.completer.complete(cline, cpoint)
