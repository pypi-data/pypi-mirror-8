<?xml version="1.0"?>
<object
    name="portal_controlpanel"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="ityou.astream"
    purge="False">

    <configlet
        title="ITYOU ESI - Activity Stream"
        action_id="astream"
        appId="ityou.astream"
        category="Products"
        condition_expr=""
        url_expr="string:${portal_url}/@@astream-settings"
        icon_expr="string:${portal_url}/++resource++ityou.astream.images/ityou-esi-astream-icon.png"
        visible="True"
        i18n:attributes="title">
            <permission>Manage portal</permission>
    </configlet>

</object>
