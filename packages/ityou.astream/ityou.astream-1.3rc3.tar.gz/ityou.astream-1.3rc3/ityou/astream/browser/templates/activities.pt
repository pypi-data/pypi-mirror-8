<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      lang="en"
      metal:use-macro="here/main_template/macros/master"
      i18n:domain="ityou.astream">
<body>

    <div metal:fill-slot="main">
    <metal:main-macro define-macro="main"
                      tal:define="kssClassesView context/@@kss_field_decorator_view;
                                  getKssClasses nocall:kssClassesView/getKssClassesInlineEditable;
                                  templateId template/getId;
                                  is_mobile python:False;
                                  ">
        <div class="ajax-params invisible" id="JESI-PARAMS">
		  <div id="ASTREAM_DELAY" tal:content="view/ASTREAM_DELAY"></div>
		  <div id="ESI_ROOT"      tal:content="view/ESI_ROOT"></div>
		  <div id="JESI-VIEW"     data-ityou-view="activity-stream" 
		                          tal:attributes="data-ityou-template templateId;
                                                  data-ityou-uid   view/uid | string:"  ></div>
    	</div>
    	
        <div metal:use-macro="view/context/ityou.astream.macros/astream-macro" />
        <div metal:use-macro="view/context/ityou.astream.tmplmacros/astream-tmpl-macro" />
        
        <div class="visualClear"></div>

        <div id="debug" class="debug"></div>
         
    </metal:main-macro>
    </div>
</body>
</html>
