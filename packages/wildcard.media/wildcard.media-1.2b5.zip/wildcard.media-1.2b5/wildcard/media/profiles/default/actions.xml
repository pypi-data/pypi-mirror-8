<?xml version="1.0"?>
<object name="portal_actions" meta_type="Plone Actions Tool"
   xmlns:i18n="http://xml.zope.org/namespaces/i18n">
 <object name="object_buttons" meta_type="CMF Action Category">
  <object name="media_convert" meta_type="CMF Action">
   <property name="title">Convert Video</property>
   <property name="description"></property>
   <property
      name="url_expr">string:${object_url}/@@convert-video</property>
   <property name="link_target"></property>
   <property name="icon_expr"></property>
   <property
      name="available_expr">object/@@wcmedia-utils/valid_type</property>
   <property name="permissions">
    <element value="Modify portal content"/>
   </property>
   <property name="visible">True</property>
  </object>
 </object>
</object>
