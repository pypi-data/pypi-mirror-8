<?xml version="1.0"?>
<object
    name="portal_controlpanel"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="ityou.notify"
    purge="False">

    <configlet
        title="ITYOU ESI - Notify"
        action_id="notify"
        appId="ityou.notify"
        category="Products"
        condition_expr=""
        url_expr="string:${portal_url}/@@notify-settings"
        icon_expr="string:${portal_url}/++resource++ityou.notify.images/ityou-esi-notify-icon.png"
        visible="True"
        i18n:attributes="title">
            <permission>Manage portal</permission>
    </configlet>

</object>
