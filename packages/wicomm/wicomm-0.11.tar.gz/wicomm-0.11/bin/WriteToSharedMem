#!/usr/bin/env python
from wicomm.gateway.sharedmem import SharedMemoryWriter
import time

def main():
    # write data to shared memory at this rate
    WRITE_FREQ = 25
    sharedMemWriter = SharedMemoryWriter()
    while(True):
        sharedMemWriter.update()
        time.sleep(1.0/WRITE_FREQ)

if __name__ == '__main__':
    main()
