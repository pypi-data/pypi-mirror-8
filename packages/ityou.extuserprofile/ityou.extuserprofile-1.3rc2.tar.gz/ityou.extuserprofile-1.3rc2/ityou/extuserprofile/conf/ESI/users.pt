<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      lang="en"
      metal:use-macro="here/main_template/macros/master"
      i18n:domain="ityou.extuserprofile">

<body>
    <div metal:fill-slot="main">
    <!-- templates/users.pt -->
    <metal:main-macro define-macro="main"
                      tal:define="kssClassesView context/@@kss_field_decorator_view;
                                  getKssClasses nocall:kssClassesView/getKssClassesInlineEditable;
                                  templateId template/getId;
                                  is_mobile python:False;
                                  ">
        <div id="ESI_ROOT"      tal:content="view/ESI_ROOT"></div>


	    <div id ="user-list-in" class="userlisting">
            <h1 tal:condition="nothing" class="documentFirstHeading" i18n:translate="contacts">Kontakte</h1>
            <table  id ="user-table" class="table table-responsive">
                <thead>
                    <tr>
                        <th class="recent_time hidden"></th>
                                <th class="portrait" i18n:translate="portait">Portr.</th>
                                <th class="fullname" i18n:translate="lastname_name">Name</th>
                                <th class="send-message"></th>
                        <th class="position" i18n:translate="position">Position</th>
                        <th class="phone" i18n:translate="phone">Phone number</th>
                        <th class="location" i18n:translate="Location">Location</th>
                        <th class="lastlogin" i18n:translate="last_login">Last login</th>
                    </tr>
                </thead>   
                <tbody>
                </tbody>
            </table>
        </div>                    

    </metal:main-macro>
    </div>
</body>
</html>
