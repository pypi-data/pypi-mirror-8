<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      lang="en"
      i18n:domain="ityou.extuserprofile">
<body>

<!-- MACROS ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
<metal:macro define-macro="fieldset" tal:condition="python: (my_property or f[5]) and (my_property or u[f[0]])">

            <div tal:attributes="class python:'property ' + f[2]">       
                <label tal:attributes="class python:f[0]" i18n:translate=""><span tal:replace="python:f[1]" /></label>

                <div tal:condition="python: u[f[0]] and not my_property" 
                     tal:attributes="id python:f[0]; class python:f[4]" ><span 
                           tal:condition="python:not f[3]"
                           tal:replace="python:u[f[0]]"  /><a 
                           tal:condition="python:f[3]"
                           tal:attributes="href python:f[3] % u[f[0]]"
                           tal:content="python:u[f[0]]" /></div>

				<div tal:condition="python: my_property and f[4] == 'CheckBox'"
					tal:attributes="id python:f[0]; class python:'pfield '+f[4]" tabindex="200">
					<i class="fa fa-check-square-o" tal:condition="python: u[f[0]]"></i>
					<i class="fa fa-square-o" tal:condition="python: not u[f[0]]"></i>
				</div>

                <div tal:condition="python: my_property and f[4] != 'CheckBox'" 
                     tal:attributes="id python:f[0]; class python:'pfield '+f[4]" tabindex="200"><span 
                           tal:replace="python:u[f[0]]" /></div>
            </div>



</metal:macro>
<!-- /MACROS ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->


</body>
</html>
