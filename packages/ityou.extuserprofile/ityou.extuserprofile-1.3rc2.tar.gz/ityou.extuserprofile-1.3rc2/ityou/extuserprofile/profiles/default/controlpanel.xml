<?xml version="1.0"?>
<object
    name="portal_controlpanel"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="ityou.extuserprofile"
    purge="False">

    <configlet
        title="ITYOU ESI - Extended user profile"
        action_id="extuserprofile"
        appId="ityou.extuserprofile"
        category="Products"
        condition_expr=""
        url_expr="string:${portal_url}/@@userprofile-settings"
        icon_expr="string:${portal_url}/++resource++ityou.extuserprofile.images/ityou-esi-extuserprofile-icon.png"
        visible="True"
        i18n:attributes="title">
            <permission>Manage portal</permission>
    </configlet>

</object>
