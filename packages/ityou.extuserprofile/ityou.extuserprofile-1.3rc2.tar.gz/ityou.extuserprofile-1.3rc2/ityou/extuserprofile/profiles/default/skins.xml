<?xml version="1.0"?>
<object name="portal_skins" allow_any="False" cookie_persistence="False">
   
 <object name="ityou_extuserprofile_templates"
    meta_type="Filesystem Directory View"
    directory="ityou.extuserprofile:skins/templates"/>

 <object name="ityou_extuserprofile_images"
    meta_type="Filesystem Directory View"
    directory="ityou.extuserprofile:skins/images"/>

 <skin-path name="*">
  <layer name="ityou_extuserprofile_images" insert-after="custom"/>
  <layer name="ityou_extuserprofile_templates" insert-after="custom"/>
 </skin-path>
 
</object>
