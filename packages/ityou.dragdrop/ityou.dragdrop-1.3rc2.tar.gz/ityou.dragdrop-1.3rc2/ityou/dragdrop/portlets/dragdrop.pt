<dl class="portlet portletDragdrop portlet-static-drag-drop"
    i18n:domain="ityou.dragdrop" >
    <dt class="portletHeader">
        <span class="portletTopLeft"></span>
        <span i18n:translate="drag_and_drop">Drag & Drop</span>
        <span class="portletTopRight"></span>
    </dt>
    
<dd class="portletItem odd">
    <p id="dragdrop-container">
        <span id="dragdrop-portlet-text" i18n:translate="drop_docs_here">Drop Documents here to copy or move.</span>
    </p>
        
    <span class="portletBottomLeft"></span>
    <span class="portletBottomRight"></span>
        
    <div class="copy-move-container border-box border-radius clear-fix-drag-drop">
    	<button class="action-bar border-box copy" i18n:translate="copy">Copy</button>
    	<button class="action-bar border-box move state-active" i18n:translate="move">Move</button>
    </div>
    
    <div id="delete-container">
    	<a tal:attributes="href view/trash_folder">
        	<i class="fa fa-trash-o"></i>
        </a>
    </div>
</dd>

    <dd id="debug-imessage" class="debug"></dd>
    
    
    
    <!--  ================= JQ-TMPL ================= -->

        <tal:comment condition="nothing">
        # ------------------------------------------------------------------------------
        #
        # Template für Objekte im Drag&Drop Portlet
        # 
        # Element: {
        #   author: {String},
        #   created: {String},
        #   description: {String},
        #   edited: {String},
        #   icon: {String},
        #   id: {String},
        #   portal_type: {String},
        #   thumbnail_class: {String},
        #   thumbnail_url: {String},
        #   title: {String},
        #   uid: {String},
        #   url: {String}
        # }
        #
        #
        # Aufruf in `ityou.esi.theme/src/ityou/esi/theme/esi_theme/jesi-assets/js/ityou/ityou.dragdrop.js:184`
        # Aufruf in `ityou.esi.theme/src/ityou/esi/theme/esi_theme/jesi-assets/js/ityou/ityou.dragdrop.js:699`
        #
        # ------------------------------------------------------------------------------
        </tal:comment>
        <script type="text/x-jsrender" id="dragdrop-dragdropcontainer-tmpl">
            
            <div class="drag-drop-container border-box border-radius clearfix" id="{{:uid}}"
                data-id="{{:id}}"
                data-uid="{{:uid}}"
                data-portal-type="{{:portal_type}}"
                data-url="{{:url}}"
                data-thumbnail="{{:thumbnail_class}}"
                data-contenticon="{{:icon}}"
                data-addinfo-author="{{:author}}"
                data-addinfo-date-created="{{:created}}"
                data-addinfo-date-edited="{{:edited}}"
                data-addinfo-description="{{:description}}"
            >
                <div class="action-bar border-box clearfix">
                    <button class="copy-move-item fa fa-arrow-left"></button>
                    <input type="checkbox" name="{{:uid}}">
                    <span class="loader fa fa-spinner fa-spin"></span>
                    <span class="delete-item fa fa-trash-o"></span>
                </div>
                
                <div class="thumbnail-wrapper border-box">
                    <img src="{{:thumbnail_url}}" class="img-thumbnail">
                </div>
                
                <div class="item-title border-box break-word">{{:title}}</div>
            </div>
            
        </script>
    

