from setuptools import setup

setup(
    name="{{name}}",
    version="{{version}}",
    author="{{author}}",
    author_email="{{author_email}}",
    {% if description %}
    description="{{description}}",
    {% endif %}
    {% if url %}
    url="{{url}}",
    {% endif %}
    {% if test_suite %}
    test_suite="{{test_suite}}",
    {% endif %}
    {% if license %}
    license="{{license}}",
    {% endif %}
)
