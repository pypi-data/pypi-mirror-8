<input type="text" id="" name="" lang="" autocomplete="off"
	data-ztfy-widget="multiselect"
	data-multiselect-class="directory"
	data-multiselect-search-callback="$.ZTFY.reference.search"
	data-multiselect-max-results="20"
	data-multiselect-enable-new="false"
	data-multiselect-complex-search="false"
	data-multiselect-max-selection-length="1"
	tal:attributes="id view/id;
					name view/name;
					lang view/lang;
					class view/klass;
					style view/style;
					title view/title;
					value view/value;
					disabled view/disabled;
					onclick view/onclick;
					ondblclick view/ondblclick;
					onmousedown view/onmousedown;
					onmouseup view/onmouseup;
					onmouseover view/onmouseover;
					onmousemove view/onmousemove;
					onmouseout view/onmouseout;
					onkeypress view/onkeypress;
					onkeydown view/onkeydown;
					onkeyup view/onkeyup;
					tabindex view/tabindex;
					onfocus view/onfocus;
					onblur view/onblur;
					onchange view/onchange;
					readonly view/readonly;
					alt view/alt;
					accesskey view/accesskey;
					onselect view/onselect;
					size view/size;
					maxlength view/maxlength;
					data-multiselect-references string:{ '${view/value}': '${view/target_title/text:js}' };" />