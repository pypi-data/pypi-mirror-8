<tal:var define="ignore python:request.response.setHeader('Cache-Control', 'no-cache, no-store')" />
<tal:var replace="structure provider:z3c.formjs.interfaces.IDynamicJavaScript" />
<div class="dialog" i18n:domain="ztfy.skin"
	 tal:attributes="data-ztfy-url request/URL">
	<div id="osx-container">
		<div id="osx-modal-title" tal:content="view/label | nothing" i18n:translate="" />
		<a class="hint close icon icon-remove" title="Close dialog" i18n:attributes="title"
		   href="javascript:$.ZTFY.dialog.close();"></a>
	</div>
	<div class="data">
		<h1 tal:content="view/title | context/i18n:title | nothing" i18n:translate="">Title</h1>
		<tal:var content="structure provider:pagelet">Page content</tal:var>
	</div>
</div>
