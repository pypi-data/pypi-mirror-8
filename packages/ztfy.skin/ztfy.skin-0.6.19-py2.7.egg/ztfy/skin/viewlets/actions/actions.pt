<div class="actions" tal:define="viewlets view/viewlets" tal:condition="viewlets" i18n:domain="ztfy.skin">
	<h2 i18n:translate="">Settings</h2>
	<tal:var define="global url python:None">
		<tal:loop repeat="viewlet viewlets">
			<tal:if define="rendered viewlet/render"
					condition="python:rendered and (viewlet.url != url)">
				<tal:var replace="structure rendered" />
				<tal:var define="global url viewlet/url" />
			</tal:if>
		</tal:loop>
	</tal:var>
</div>
