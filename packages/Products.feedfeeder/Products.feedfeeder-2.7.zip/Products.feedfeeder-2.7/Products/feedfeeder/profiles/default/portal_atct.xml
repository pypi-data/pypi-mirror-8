<?xml version="1.0"?>
<atcttool
    xmlns:i18n="http://xml.zope.org/namespaces/i18n">
  <topic_indexes i18n:domain="plone">
    <index name="getFeedItemUpdated"
           description="Date that the FeedItem was last updated."
           enabled="True"
           friendlyName="FeedItem Updated"
           i18n:attributes="description; friendlyName">
      <criteria>ATFriendlyDateCriteria</criteria>
      <criteria>ATDateRangeCriterion</criteria>
    </index>
  </topic_indexes>
  <topic_metadata i18n:domain="plone">
    <metadata
        name="getFeedItemUpdated"
        description="Date that the FeedItem was last updated."
        enabled ="True"
        i18n:attributes="description"
        />
  </topic_metadata>
</atcttool>
