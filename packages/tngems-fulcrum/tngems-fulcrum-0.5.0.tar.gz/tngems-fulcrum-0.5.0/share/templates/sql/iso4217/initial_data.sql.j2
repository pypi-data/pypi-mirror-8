        alphabetic_code = U1.alphabetic_code,
        numeric_code = U1.numeric_code,
        minor_unit = COALESCE(U1.minor_unit, -1),
        during = daterange(U1.valid_from::date, COALESCE(U1.valid_to::date, 'infinity'))
