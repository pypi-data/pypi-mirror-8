<configure
	xmlns="http://namespaces.zope.org/zope"
	i18n_domain="ztfy.file">

	<utility
		name="application/zip"
		factory=".zip.ZipArchiveExtractor" />

	<utility
		name="application/x-tar"
		factory=".tar.TarArchiveExtractor" />

	<utility
		name="application/x-gzip"
		factory=".gz.GZipArchiveExtractor" />

	<utility
		name="application/x-bzip2"
		factory=".bz2.BZip2ArchiveExtractor" />

</configure>