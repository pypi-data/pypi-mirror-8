#!python
# EASY-INSTALL-ENTRY-SCRIPT: 'falcon==0.1.10','console_scripts','falcon-bench'
__requires__ = 'falcon==0.1.10'
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.exit(
        load_entry_point('falcon==0.1.10', 'console_scripts', 'falcon-bench')()
    )
