#: ztfy/extfile/extfile.py:161 ztfy/extfile/extfile.py:211
msgid "You have to define an IExtFileHandler utility to use external files !"
msgstr ""
"Vous devez définir un utilitaire de type IExtFileHandler pour utiliser des "
"fichiers externes !"

#: ztfy/extfile/handler/local/interfaces.py:40
#: ztfy/extfile/handler/sftp/interfaces.py:40
msgid "Handler config name"
msgstr "Nom de la configuration"

#: ztfy/extfile/handler/local/interfaces.py:41
#: ztfy/extfile/handler/sftp/interfaces.py:41
msgid "Name with which the utility is registered"
msgstr "Nom sous lequel l'utilitaire a été enregistré'"

#: ztfy/extfile/handler/local/interfaces.py:45
msgid "Local root"
msgstr "Racine locale"

#: ztfy/extfile/handler/local/interfaces.py:46
msgid "Local root of ExtFile files, added to namechooser base path"
msgstr ""
"Racine de stockage des fichiers externes, ajouté automatiquement au nom "
"généré par le composant 'NameChooser'"

#: ztfy/extfile/handler/sftp/interfaces.py:45
msgid "Host name"
msgstr "Nom de l'hôte"

#: ztfy/extfile/handler/sftp/interfaces.py:46
msgid "SFTP server name"
msgstr "Nom du serveur SFTP"

#: ztfy/extfile/handler/sftp/interfaces.py:50
msgid "Host port number"
msgstr "Port de l'hôte"

#: ztfy/extfile/handler/sftp/interfaces.py:51
msgid "SFTP server port number"
msgstr "Port du serveur SFTP"

#: ztfy/extfile/handler/sftp/interfaces.py:55
msgid "User name"
msgstr "Code utilisateur"

#: ztfy/extfile/handler/sftp/interfaces.py:56
msgid "SFTP session user name"
msgstr "Code utilisateur de la session SFTP"

#: ztfy/extfile/handler/sftp/interfaces.py:59
msgid "User password"
msgstr "Mot de passe"

#: ztfy/extfile/handler/sftp/interfaces.py:60
msgid "SFTP session user password"
msgstr "Mot de passe de la session SFTP"

#: ztfy/extfile/handler/sftp/interfaces.py:63
msgid "Private key file"
msgstr "Fichier de clé privée"

#: ztfy/extfile/handler/sftp/interfaces.py:64
msgid "Path name to private key file"
msgstr "Chemin d'accès vers la clé privée'"

#: ztfy/extfile/handler/sftp/interfaces.py:68
msgid "Remote root"
msgstr "Racine distante"

#: ztfy/extfile/handler/sftp/interfaces.py:69
msgid "Remote root of ExtFile files"
msgstr ""
"Racine du point de stockage des fichiers externes sur le serveur distant"

#: ztfy/extfile/handler/sftp/interfaces.py:73
msgid "Cache root"
msgstr "Racine du cache"

#: ztfy/extfile/handler/sftp/interfaces.py:74
msgid "Local root of cached files"
msgstr "Racine locale du point de stockage du cache des fichiers externes"

#: ztfy/extfile/handler/sftp/metaconfigure.py:52
msgid "You have to define private_key file or password !"
msgstr ""
"Vous devez définir le nom du fichier contenant la clé privée ou le mot de "
"passe !"

#: ztfy/extfile/interfaces.py:38
msgid "External file name"
msgstr "Nom du fichier externe"

#: ztfy/extfile/interfaces.py:39
msgid "Identify the file name on the file system"
msgstr "Identification du fichier externe sur le système de fichiers"

#: ztfy/extfile/namechooser/interfaces.py:37
msgid "Configuration name"
msgstr "Nom de la configuration"

#: ztfy/extfile/namechooser/interfaces.py:38
msgid "Name of the ExtFile name chooser configuration"
msgstr ""
"Nom sous lequel l'utilitaire de génération des noms de fichiers est "
"enregistré"

#: ztfy/extfile/namechooser/interfaces.py:42
msgid "Temporary path name"
msgstr "Répertoire temporaire"

#: ztfy/extfile/namechooser/interfaces.py:43
msgid "Absolute path of temporary files"
msgstr ""
"Répertoire de stockage des fichiers temporaires avant transfert vers le lieu "
"de stockage final"

#: ztfy/extfile/namechooser/interfaces.py:47
msgid "Base path name"
msgstr "Répertoire de base"

#: ztfy/extfile/namechooser/interfaces.py:48
msgid "Absolute base path of final files"
msgstr "Racine du stockage définitif des fichiers externes"

#: ztfy/extfile/namechooser/interfaces.py:52
msgid "Filename chooser class"
msgstr "Classe de générateur de noms"

#: ztfy/extfile/namechooser/interfaces.py:53
msgid ""
"Name of an external files names chooser, implementing IExtFileNameChooser"
msgstr ""
"Nom d'un générateur de noms, implémentant l'interface IExtFileNameChooser"
