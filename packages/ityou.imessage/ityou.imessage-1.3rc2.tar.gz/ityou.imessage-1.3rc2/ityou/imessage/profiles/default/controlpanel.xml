<?xml version="1.0"?>
<object
    name="portal_controlpanel"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="ityou.imessage"
    purge="False">

    <configlet
        title="ITYOU ESI - Instant Messaging"
        action_id="imessage"
        appId="ityou.imessage"
        category="Products"
        condition_expr=""
        icon_expr="string:${portal_url}/++resource++ityou.imessage.images/ityou-esi-imessage-icon.png"
        url_expr="string:${portal_url}/@@imessage-settings"
        visible="True"
        i18n:attributes="title">
            <permission>Manage portal</permission>
    </configlet>

</object>

