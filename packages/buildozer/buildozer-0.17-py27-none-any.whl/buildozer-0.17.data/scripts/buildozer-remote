#!python
# EASY-INSTALL-ENTRY-SCRIPT: 'buildozer==0.17','console_scripts','buildozer-remote'
__requires__ = 'buildozer==0.17'
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.exit(
        load_entry_point('buildozer==0.17', 'console_scripts', 'buildozer-remote')()
    )
