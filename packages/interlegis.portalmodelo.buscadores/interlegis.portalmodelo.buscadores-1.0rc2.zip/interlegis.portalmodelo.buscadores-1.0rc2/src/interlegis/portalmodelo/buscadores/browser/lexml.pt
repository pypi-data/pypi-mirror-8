<html xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      lang="pt_BR"
      metal:use-macro="context/main_template/macros/master"
      i18n:domain="interlegis.portalmodelo.buscadores">
<body>
  <metal:results fill-slot="content">
    <h1 class="documentFirstHeading">
        Resultado da pesquisa no LexML
    </h1>

    <div id="content-core">
      <iframe width="100%" height="500px" title="Resultado da pesquisa no LexML"
          src="http://www.lexml.gov.br/busca/search?keyword=keyword"
          tal:attributes="src view/iframe_src">
      </iframe>
    </div>
  </metal:results>
</body>
</html>
