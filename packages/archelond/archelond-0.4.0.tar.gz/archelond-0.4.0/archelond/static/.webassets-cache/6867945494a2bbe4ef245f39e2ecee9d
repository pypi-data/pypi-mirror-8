V(function($){var Command=Backbone.Model.extend({last_used:function(){stamp=this.get('timestamp');if(typeof stamp!=='undefined'){date=new Date(stamp);return date.toLocaleDateString()+' '+date.toLocaleTimeString();}else{return''}},});var Commands=Backbone.Collection.extend({model:Command,url:HISTORY_API_URL,parse:function(response){return response.commands;}});var CommandsView=Backbone.View.extend({el:'#history-table-body',render:function(){var source=$('#CommandsTemplate').html();var template=Handlebars.compile(source);var html=template(this.collection.models);this.$el.html(html);},afterRender:function(){$('#history-table').dataTable({'filter':false,'destroy':true});},initialize:function(){this.collection=new Commands()\u000athis.listenTo(this.collection,'add',this.render);this.listenTo(this.collection,'reset',this.render);this.collection.on('add',this.render,this);this.collection.on('render',this.afterRender)\u000athis.collection.fetch();}});var commands=new CommandsView();})(jQuery);
