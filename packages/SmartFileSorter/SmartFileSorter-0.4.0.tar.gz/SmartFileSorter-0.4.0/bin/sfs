#!/usr/bin/env python

# Smart File Sorter
# Rule-based file moving and renaming

# For help, run sfs --help or see http://github.com/jashort/SmartFileSorter/

import smartfilesorter

sfp = smartfilesorter.SmartFileSorter()
sfp.args = sfp.parse_arguments()
sfp.create_logger(sfp.args)
sfp.run(sfp.args)

