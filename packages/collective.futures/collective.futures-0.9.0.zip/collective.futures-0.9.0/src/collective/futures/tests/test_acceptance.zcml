<configure
    xmlns="http://namespaces.zope.org/zope"
    xmlns:browser="http://namespaces.zope.org/browser"
    i18n_domain="collective.futures">

  <browser:page
      name="testing-result"
      for="*"
      class=".test_acceptance.ResultView"
      template="test_acceptance.pt"
      permission="zope2.View"
      />

  <browser:page
      name="testing-result-or-submit"
      for="*"
      class=".test_acceptance.ResultOrSubmitView"
      template="test_acceptance.pt"
      permission="zope2.View"
      />

  <browser:page
      name="testing-result-or-submit-placeholder"
      for="*"
      class=".test_acceptance.ResultOrSubmitPlaceholderView"
      template="test_acceptance.pt"
      permission="zope2.View"
      />

  <browser:page
      name="testing-result-multiprocess"
      for="*"
      class=".test_acceptance.ResultMultiprocessView"
      template="test_acceptance.pt"
      permission="zope2.View"
      />

  <browser:page
      name="testing-result-or-submit-multiprocess"
      for="*"
      class=".test_acceptance.ResultOrSubmitMultiprocessView"
      template="test_acceptance.pt"
      permission="zope2.View"
      />

  <browser:page
      name="testing-transaction-abort"
      for="*"
      class=".test_acceptance.TransactionAbortView"
      template="test_acceptance.pt"
      permission="zope2.View"
      />

  <browser:page
      name="testing-transaction-commit"
      for="*"
      class=".test_acceptance.TransactionCommitView"
      template="test_acceptance.pt"
      permission="zope2.View"
      />

</configure>
