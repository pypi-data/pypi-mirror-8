<?xml version="1.0"?>
<object
    name="portal_controlpanel"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="ityou.whoisonline"
    purge="False">

    <configlet
        title="ITYOU ESI - Who is online?"
        action_id="whoisonline"
        appId="ityou.whoisonline"
        category="Products"
        condition_expr=""
        url_expr="string:${portal_url}/@@whoisonline-settings"
        visible="True"
        icon_expr="string:${portal_url}/++resource++ityou.whoisonline.resources/ityou-esi-whoisonline-icon.png"
        i18n:attributes="title">
            <permission>Manage portal</permission>
    </configlet>

</object>

