<?xml version="1.0"?>
<object name="mimetypes_registry" meta_type="MimeTypes Registry">

  <mimetype name="Comma Separated Values"
            mimetypes="text/comma-separated-values
                       text/csv"
            extensions="csv"
            globs="*.csv"
            icon_path="++resource++collective.tinymceplugins.advfilelinks.resources/csv.png"
            binary="False"
            />

  <mimetype name="Zip archive"
            mimetypes="application/zip
                       application/x-zip-compressed"
            extensions="zip"
            globs="*.zip"
            binary="True"
            icon_path="++resource++collective.tinymceplugins.advfilelinks.resources/zip.png"
            />

 <mimetype name="RAR archive"
           mimetypes="application/x-rar"
		   extensions="rar"
		   globs="*.rar"
		   binary="True"
           icon_path="++resource++collective.tinymceplugins.advfilelinks.resources/zip.png" />

</object>
