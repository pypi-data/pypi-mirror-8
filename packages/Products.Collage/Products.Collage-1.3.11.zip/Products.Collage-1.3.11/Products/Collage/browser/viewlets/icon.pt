<div xmlns="http://www.w3.org/1999/xhtml" xml:lang="en" lang="en"
     xmlns:tal="http://xml.zope.org/namespaces/tal"
     xmlns:metal="http://xml.zope.org/namespaces/metal"
     i18n:domain="plone"
     tal:omit-tag="">

  <span class="icon">
    <img height="16"
         width="16"
         tal:attributes="class python: view.isAlias() and 'alias' or None;
                         src string:${view/portal_url}/${view/getIcon};
                         alt here/portal_type"
         i18n:attributes="alt" />
  </span>

</div>
