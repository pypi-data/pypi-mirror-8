#!python
# EASY-INSTALL-ENTRY-SCRIPT: 'bumpversion==0.5.1','console_scripts','bumpversion'
__requires__ = 'bumpversion==0.5.1'
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.exit(
        load_entry_point('bumpversion==0.5.1', 'console_scripts', 'bumpversion')()
    )
