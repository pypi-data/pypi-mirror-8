{% extends 'base.jinja2' %}

{% block html_title %}Concept schemes{% endblock %}

{% block content %}
    <div class="row">
        <div class="large-12 columns">
            <h1 class="concept">{% trans %}select_scheme{% endtrans %}</h1>
                <ul>
                {% for item in conceptschemes %}
                  <li><a href='{{ item.uri }}'>{{ item.id }}</a></li>
                {% endfor %}
                </ul>
        </div>
    </div>
{% endblock %}