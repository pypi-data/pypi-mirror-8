<?xml version="1.0"?>
<object
    name="portal_controlpanel"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="ityou.thumbnails"
    purge="False">

    <configlet
        title="ITYOU ESI - Thumbnails"
        action_id="Thumbnails"
        appId="ityou.thumbnails"
        category="Products"
        condition_expr=""
        url_expr="string:${portal_url}/@@thumbnail-settings"
        icon_expr="string:${portal_url}/++resource++ityou.thumbnails.images/ityou-esi-thumbnails-icon.png"
        visible="True"
        i18n:attributes="title">
            <permission>Manage portal</permission>
    </configlet>

</object>
