# Ubuntu sources.list template file
#
# You have to use the environment variable {DIST} in this file instead of a
# fixed codename to permit distribution name substitution
#
# Content of this file will be aggregated to /etc/apt/sources.list.d/lgp.list
# in chroot

deb http://security.ubuntu.com/ubuntu ${DIST}-security main restricted
deb http://archive.ubuntu.com/ubuntu ${DIST}-updates main restricted

