<?xml version="1.0"?>
<object name="portal_css">
  <!-- These are the Sunburst-styles which we want to include in our custom
       theme. They are configured to be in the "default" bundle.
       Note: by default, all styles land in the "default" bundle. So this
       configuration isn't strictly necessary. -->
  <stylesheet bundle="default" id="diazo_sunburst_authoring.css"/>
  <stylesheet bundle="default" id="diazo_sunburst_base.css"/>
  <stylesheet bundle="default" id="diazo_sunburst_controlpanel.css"/>
  <stylesheet bundle="default" id="diazo_sunburst_forms.css"/>
  <stylesheet bundle="default" id="diazo_sunburst_IEFixes.css"/>
  <stylesheet bundle="default" id="diazo_sunburst_member.css"/>
  <stylesheet bundle="default" id="diazo_sunburst_reset.css"/>
  <stylesheet bundle="default" id="diazo_sunburst_dropdown-menu.css"/>
</object>
