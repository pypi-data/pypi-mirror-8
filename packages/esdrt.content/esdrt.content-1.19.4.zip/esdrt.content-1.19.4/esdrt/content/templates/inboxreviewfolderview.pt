                <tr>
                  <th>
                    Observation
                  </th>
                  <th>
                    CRF code
                  </th>   
                  <th style="width:20px">
                    Inventory year
                  </th> 
                  <th style="width:80px">
                    Modification date
                  </th>
                  <th style="width:130px">
                    Progress
                  </th>
                  <th>
                    Step
                  </th>
                </tr>
              </thead>
