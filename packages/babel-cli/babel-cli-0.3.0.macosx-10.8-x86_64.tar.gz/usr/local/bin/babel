#!/usr/local/opt/python/bin/python2.7
# EASY-INSTALL-ENTRY-SCRIPT: 'babel-cli==0.3.0','console_scripts','babel'
__requires__ = 'babel-cli==0.3.0'
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.exit(
        load_entry_point('babel-cli==0.3.0', 'console_scripts', 'babel')()
    )
