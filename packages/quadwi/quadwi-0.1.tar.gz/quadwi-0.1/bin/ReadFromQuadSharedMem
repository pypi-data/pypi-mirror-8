#!/usr/bin/python
from quadwi import QuadSharedMemReader
import time

# write data to shared memory at this rate
READ_FREQ = 10

def main():
    quadReader = QuadSharedMemReader()
    while(True):
        print quadReader.readData()
        time.sleep(1.0/READ_FREQ)

if __name__ == '__main__':
    main()
