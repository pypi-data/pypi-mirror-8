#!/usr/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'PiFi==1.1.0','console_scripts','pifi-display'
__requires__ = 'PiFi==1.1.0'
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.exit(
        load_entry_point('PiFi==1.1.0', 'console_scripts', 'pifi-display')()
    )
