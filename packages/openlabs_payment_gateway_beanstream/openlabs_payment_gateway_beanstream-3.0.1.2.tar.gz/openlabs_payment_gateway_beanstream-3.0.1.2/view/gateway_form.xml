<data>
    <xpath expr="/form/notebook" position="inside">
        <page string="Beanstream Settings" id="beanstream"
          states="{'invisible': Eval('provider') != 'beanstream'}">
            <group colspan="4" col="6" id="basic_config">
                <label name="beanstream_auth_mechanism"/>
                <field name="beanstream_auth_mechanism"/>
                <label name="beanstream_currency"/>
                <field name="beanstream_currency"/>
                <label name="beanstream_merchant_id"/>
                <field name="beanstream_merchant_id"/>
            </group>
            <label name="beanstream_pass_code"/>
            <field name="beanstream_pass_code"/>
            <newline/>
            <label name="beanstream_hash_key"/>
            <field name="beanstream_hash_key"/>
        </page>
    </xpath>
</data>

