#!/usr/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'pyrev==0.35','console_scripts','pyrev-devel'
__requires__ = 'pyrev==0.35'
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.exit(
        load_entry_point('pyrev==0.35', 'console_scripts', 'pyrev-devel')()
    )
