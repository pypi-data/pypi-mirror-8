<div id="js_info" style="visibility: hidden" tal:condition="view/isPanelDisabled">
	<span tal:content="view/buttonJson" id="buttonJson">
	</span>
</div>
<div title="Hide transition buttons" id="prefsDialog" tal:define="prefsURL view/getPreferencesUrl">
	<p>
		<a class="button" tal:attributes="href prefsURL">
			<button class="button">
				Go to preferences
			</button>
		</a>
		<button class="close">Close</button>
	</p>
</div>