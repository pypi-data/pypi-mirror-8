#!python
# EASY-INSTALL-ENTRY-SCRIPT: 'shardgather==1.0.1','console_scripts','shardgather'
__requires__ = 'shardgather==1.0.1'
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.exit(
        load_entry_point('shardgather==1.0.1', 'console_scripts', 'shardgather')()
    )
