<?xml version="1.0" encoding="UTF-8"?>
<object>
    <hidden manager="plone.portalfooter" skinname="*">
        <viewlet name="plone.footer" />
        <viewlet name="plone.colophon" />
        <viewlet name="plone.analytrics" />
        <viewlet name="plone.site_actions" />
    </hidden>

    <hidden manager="plone.portalheader" skinname="*">
        <viewlet name="plone.skip_links" />
        <viewlet name="plone.languageselector" />
        <viewlet name="plone.searchbox" />
    </hidden>

    <hidden manager="plone.abovecontent" skinname="*">
        <viewlet name="plone.path_bar" />
    </hidden>

    <hidden manager="plone.belowcontenttitle" skinname="*">
        <viewlet name="plone.belowcontenttitle.documentbyline" />
    </hidden>

    <hidden manager="plone.belowcontent" skinname="*">
        <viewlet name="plone.manage_portlets_fallback" />
    </hidden>

    <order manager="plone.portalfooter" skinname="*">
        <viewlet name="plonetheme.bootstrapModern.footer" insert-after="*"/>
    </order>

    <order manager="plone.portalheader" skinname="*">
        <viewlet name="plonetheme.bootstrapModern.sections" insert-before="*"/>
    </order>

</object>
